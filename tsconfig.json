{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "types": ["node"],
    "moduleResolution": "NodeNext",
    "allowSyntheticDefaultImports": true,
    "module": "NodeNext",
    "target": "ESNext",
    "baseUrl": ".",
    "paths": {
      "csv-parse/*": ["node_modules/csv-parse/*"]
    }
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules"]
}
